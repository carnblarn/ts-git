{
    "compilerOptions": {
        "lib": ["es2020"],
        "module": "commonjs",
        "target": "es2019",
        "outDir": "dist",
        "noEmitOnError": false,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    },
    "include": ["."]
}
